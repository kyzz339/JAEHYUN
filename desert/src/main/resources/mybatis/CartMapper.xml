<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.desert.demo.mapper.CartMapper">
	
	<select id="listCartDao" resultType="com.desert.demo.dto.DTOCart">
		select 
			cart.goods_idx, goods.idx 
		from 
			cart join goods
		on 
			cart.goods_idx = goods.idx
	</select>
	
